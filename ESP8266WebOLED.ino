void handleNotFound() {
  //digitalWrite(led, 1);
  String message = "File Not Found\n\n";
  message += "URI: ";
  message += server.uri();
  message += "\nMethod: ";
  message += (server.method() == HTTP_GET) ? "GET" : "POST";
  message += "\nArguments: ";
  message += server.args();
  message += "\n";
  for (uint8_t i = 0; i < server.args(); i++) {
    message += " " + server.argName(i) + ": " + server.arg(i) + "\n";
  }
  server.send(404, "text/plain", message);
  //digitalWrite(led, 0);
}

void AnimateTalk(int syllables){
  long randWait = 0; 
  for(int x = 1; x<=syllables; x++){
  randWait = random(50,200); 
  drawCutie(cutieBigSmile_width, cutieBigSmile_height, cutieBigSmile_bits); 
  delay(randWait); 
  drawCutie(cutieSmallSmile_width, cutieSmallSmile_height, cutieSmallSmile_bits);
  randWait = random(50,100);
  delay(randWait); 
  drawCutie(cutieSmile_width, cutieSmile_height, cutieSmile_bits);
  randWait = random(50,200);
  delay(randWait);
  }
}

void drawCutie(int x, int y, unsigned char image[]){
    u8g2.firstPage();  
  do {
      u8g2.drawXBMP( 25, 2, x, y, image);
  } while( u8g2.nextPage() );
  delay(100); 
}

void writeMessage(String msg){
u8g2.firstPage();
  do {
    u8g2.setFont(u8g2_font_ncenB14_tr);
    u8g2.drawStr(0,15,msg.c_str());
  } while ( u8g2.nextPage() );
  delay(1000);
}

